    "version": "0.2.0",
    "configurations": [
        {
            "name": "xv6launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/src/kernel",
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/src",
            "miDebuggerPath": "/usr/bin/gdb",
            "miDebuggerServerAddress": "127.0.0.1:26000",
            "MIMode": "gdb",
            "preLaunchTask": "xv6build"
        }
    ],
}